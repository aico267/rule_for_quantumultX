{
  "log": {
    "disabled": false,
    "level": "error",
    "timestamp": true
  },
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "google",
        "address": "https://8.8.8.8/dns-query",
        "detour": "proxy"
      },
      {
        "tag": "tx",
        "address": "https://120.53.53.53/dns-query",
        "detour": "cn"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      },
      {
        "tag": "block",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "tx",
        "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "server": "tx"
      },
      {
        "clash_mode": "Global",
        "server": "fakeip"
      },
      {
        "rule_set": "Advertising",
        "server": "block"
      }
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": "ChinaMax",
        "server": "tx"
      }
    ],
    "strategy": "ipv4_only",
    "final": "google",
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "inet4_address": "172.19.0.0/30",
      "inet6_address": "fdfe:dcba:9876::0/126",
      "stack": "system",
      "auto_route": true,
      "sniff": true/*,
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "sniff": true
    }*/
  ],
  "outbounds": [
    {
      "tag": "direct",
      "type": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "tag": "dns-out",
      "type": "dns"
    },
    {
      "tag": "cn",
      "type": "selector",
      "outbounds": [
        "proxy",
        "cn-auto"
      ],
      "default": "cn-auto"
    },
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "proxy-auto"
      ],
      "default": "proxy-auto"
    },
    {
      "tag": "cn-auto",
      "type": "urltest",
      "outbounds": [],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "1m",
      "tolerance": 50
    },
    {
      "tag": "proxy-auto",
      "type": "urltest",
      "outbounds": [],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "1m",
      "tolerance": 50
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [
        "direct",
        "proxy",
        "cn"
      ],
      "default": "direct"
    }
  ],
  "route": {
    "rule_set": [
      {
        "tag": "Advertising",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/bm7/Advertising.srs",
        "download_detour": "proxy"
      },
      {
        "tag": "ChinaMax",
        "type": "remote",
        "format": "binary",
        "url": "https://github.com/MetaCubeX/meta-rules-dat/raw/sing/bm7/ChinaMax.srs",
        "download_detour": "proxy"
      }
    ],
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules":[
          {
            "port":53
          },
          {
            "protocol": "dns"
          }
        ],
        "outbound": "dns-out"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": "Advertising",
        "outbound": "block"
      },
      {
        "protocol":
        [
          "quic",
          "stun"
        ],
        "outbound": "block"
      },
      {
        "domain":
        [
          "mcdn.bilivideo.cn",
          "tracker.biliapi.net"
        ],
        "domain_regex":
        [
          "stun(.*)\\.chat\\.bilibili\\.com$",
          "(.*)-p2p-(.*)\\.chat\\.bilibili\\.com$",
          "(.*)\\.v(.*)d\\.szbdyd\\.com$",
          "(.*)-live-tracker-(.*)\\.chat\\.bilibili\\.com$"
        ],
        "ip_cidr": "203.107.1.0/24",
        "outbound": "block"
      },
      {
        "ip_version": 6,
        "outbound": "block"
      },
      {
        "rule_set": "ChinaMax",
        "outbound": "cn"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "proxy"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  }
}
